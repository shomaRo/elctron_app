  },
  "devDependencies": {
    "electron": "^34.0.2",
    "electron-builder": "^24.6.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
